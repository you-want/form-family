#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/node_modules:/Users/chenmo/Desktop/zhangbin/node_modules:/Users/chenmo/Desktop/node_modules:/Users/chenmo/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules/.pnpm/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/you-want-vue3/node_modules:/Users/chenmo/Desktop/zhangbin/lesson/node_modules:/Users/chenmo/Desktop/zhangbin/node_modules:/Users/chenmo/Desktop/node_modules:/Users/chenmo/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esbuild" "$@"
else
  exec node  "$basedir/../../bin/esbuild" "$@"
fi
