else
  export NODE_PATH="$NODE_PATH:/Users/rain9/Desktop/zhangbin/want/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/bin/node_modules:/Users/rain9/Desktop/zhangbin/want/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules/esbuild/node_modules:/Users/rain9/Desktop/zhangbin/want/you-want-vue3/node_modules/.pnpm/registry.npmmirror.com+esbuild@0.14.38/node_modules:/Users/rain9/Desktop/zhangbin/want/you-want-vue3/node_modules/.pnpm/node_modules:/Users/rain9/Desktop/zhangbin/want/you-want-vue3/node_modules:/Users/rain9/Desktop/zhangbin/want/node_modules:/Users/rain9/Desktop/zhangbin/node_modules:/Users/rain9/Desktop/node_modules:/Users/rain9/node_modules:/Users/node_modules:/node_modules"
fi
